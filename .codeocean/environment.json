{
	"version": 1,
	"base_image": "codeocean/jupyterlab:3.0.9-miniconda4.9.2-python3.8-ubuntu20.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"pip": {
			"packages": [
				{
					"name": "aind-data-access-api[docdb]",
					"version": "0.16.1"
				},
				{
					"name": "aind-data-schema",
					"version": "1.2.0"
				},
				{
					"name": "aind-data-schema-models",
					"version": "0.7.0"
				},
				{
					"name": "aind-log-utils",
					"version": "0.2.1"
				},
				{
					"name": "aws-requests-auth",
					"version": "0.4.3"
				},
				{
					"name": "boto3",
					"version": "1.35.81"
				},
				{
					"name": "kachery-cloud",
					"version": "0.4.10"
				},
				{
					"name": "matplotlib",
					"version": "3.7.5"
				},
				{
					"name": "numpy",
					"version": "1.24.4"
				},
				{
					"name": "pandas",
					"version": "2.0.3"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}